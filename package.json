{
    "name": "brighterbins",
    "label": "BrighterBins",
    "dependencies": [
        {
            "type": "service",
            "name": "http",
            "version": "v1.7.0",
            "instanceId": "shared"
        }
    ],
    "configurationBuilder": "/configurationBuilder.js",
    "configuration": [
        {
            "name": "BRIGHTERBINS_API_BASE_URL",
            "label": "BrighterBins API URL",
            "type": "label",
            "value": "https://api.brighterbins.com"
        },
        {
            "label": "Email",
            "name": "email",
            "type": "text",
            "required": true,
            "typeOptions": {
                "validation": "email"
            }
        },
        {
            "label": "Password",
            "name": "password",
            "type": "password",
            "required": true
        }
    ],
    "events": [],
    "metadata": [
        {
            "type": "script",
            "namespace": "api",
            "path": "/scripts/api.js"
        },
        {
            "type": "script",
            "namespace": "vision",
            "path": "/scripts/vision.js"
        },
        {
            "type": "script",
            "namespace": "bins",
            "path": "/scripts/bins.js"
        },
        {
            "type": "script",
            "namespace": "trackers",
            "path": "/scripts/trackers.js"
        }
    ]
}
